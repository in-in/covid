include ../button/index.pug
include ../headingDouble/index.pug
include ../text/index.pug

mixin overview()
	div(class=s.overview)&attributes(attributes)
		+headingDouble(
			{text: d.overview.heading},
			{text: d.overview.subheading})
		+text(d.overview.description)(class=s.overview_description)
		+button({text: 'Learn More'}, 'primary')
